{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "dist/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "(.*)", "has": [{ "type": "host", "value": "www.tourhab.ru" }], "status": 308, "headers": { "Location": "https://tourhab.ru$1" } },
    { "src": "/_expo/(.*)", "dest": "/dist/_expo/$1" },
    { "src": "/(.*)\\.(js|css|png|jpg|jpeg|svg|ico|map|json|txt|woff|woff2)", "dest": "/dist/$1.$2" },
    { "src": "/", "dest": "/dist/index.html" },
    { "src": "/(.+)", "dest": "/dist/$1/index.html" }
  ]
}