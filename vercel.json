{
  "version": 2,
  "public": true,
  "builds": [
    { "src": "dist/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/(.*)", "headers": { "Strict-Transport-Security": "max-age=63072000; includeSubDomains; preload" } },
    { "src": "(.*)", "has": [{ "type": "host", "value": "www.tourhab.ru" }], "status": 308, "headers": { "Location": "https://tourhab.ru$1" } },
    { "src": "/(.*)", "dest": "/dist/$1" }
  ]
}