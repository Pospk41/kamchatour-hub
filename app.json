{
  "expo": {
    "name": "kamchatka",
    "slug": "kamchatka",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "kamchatka",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "ru.kamchatka.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "10119ebe-a396-4ada-a42b-7892efa946bc"
      }
    },
    "owner": "pos-servis"
  }
}
