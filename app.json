{
  "expo": {
    "name": "kamchatka",
    "slug": "kamchatka",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "kamchatka",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "34abf0cb-fdf3-48b7-b93a-129e3ba369b0"
      }
    }
  }
}
