{
  "cli": {
    "version": "^16.0.0"
  },
  "build": {
    "dev": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_API_BASE_URL": "https://jsonplaceholder.typicode.com"
      }
    },
    "preview": {
      "developmentClient": false,
      "distribution": "internal",
      "ios": {
        "simulator": false
      }
    },
    "preview-simulator": {
      "developmentClient": false,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    }
  },
  "submit": {
    "production": {}
  }
}