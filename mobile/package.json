{
  "name": "tours-aggregator",
  "version": "0.1.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eas:login": "eas login --token $EAS_TOKEN",
    "eas:update": "eas update --branch production --message 'Auto update'",
    "eas:update:staging": "eas update --branch staging --message 'Staging update'",
    "eas:build:android": "eas build --platform android --profile production",
    "eas:build:ios": "eas build --platform ios --profile production"
  },
  "dependencies": {
    "expo": "^51.0.0",
    "react": "18.2.0",
    "react-native": "0.76.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.10.0",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "react-native-screens": "~4.6.0",
    "react-native-safe-area-context": "4.10.5",
    "@expo/vector-icons": "^14.0.0",
    "expo-secure-store": "^13.0.0",
    "expo-updates": "~0.25.28"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/react": "^18.2.66",
    "@types/react-native": "^0.73.0",
    "typescript": "~5.3.3"
  }
}
